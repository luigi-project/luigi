{
  "name": "luigi-scripts-private",
  "private": true,
  "scripts": {
    "docu": "npm run docu:container && npm run docu:client && npm run docu:core && npm run docu:element",
    "docu:generate:section": "npm run docu:client:generate:section && npm run docu:core:generate:sections",
    "docu:container": "npm run docu:container:validate && npm run docu:container:generate:sections",
    "docu:container:validate": "concurrently \"npm run docu:container:validate:container-api\" \"npm run docu:container:validate:compound-container-api\" \"npm run docu:container:validate:event-payloads\"",
    "docu:container:generate:sections": "concurrently \"npm run docu:container:generate:container-api\" \"npm run docu:container:generate:compound-container-api\" \"npm run docu:container:generate:event-payloads\"",
    "docu:container:validate:container-api": "jsdoc2md --configure ./docs/jsdoc2md.json --json ../container/typings/LuigiContainer.svelte.d.ts",
    "docu:container:generate:container-api": "jsdoc2md --partial ./docs/*.hbs --helper ./docs/mdHelpers.js --template ../docs/templates/luigi-container-api.md --separators --param-list-format=list --files ../container/typings/LuigiContainer.svelte.d.ts --configure ./docs/jsdoc2md.json > ../docs/luigi-container-api.md",
    "docu:container:validate:compound-container-api": "jsdoc2md --configure ./docs/jsdoc2md.json --json ../container/typings/LuigiCompoundContainer.svelte.d.ts",
    "docu:container:generate:compound-container-api": "jsdoc2md --partial ./docs/*.hbs --helper ./docs/mdHelpers.js --template ../docs/templates/luigi-compound-container-api.md --separators --param-list-format=list --files ../container/typings/LuigiCompoundContainer.svelte.d.ts --configure ./docs/jsdoc2md.json > ../docs/luigi-compound-container-api.md",
    "docu:container:validate:event-payloads": "jsdoc2md --configure ./docs/jsdoc2md.json --json ../container/typings/constants/events.d.ts",
    "docu:container:generate:event-payloads": "jsdoc2md --partial ./docs/*.hbs --helper ./docs/mdHelpers.js --template ../docs/templates/luigi-container-events.md --separators --param-list-format=list --files ../container/typings/constants/events.d.ts --configure ./docs/jsdoc2md.json > ../docs/luigi-container-events.md",
    "docu:client": "npm run docu:client:validate && npm run docu:client:generate:section",
    "docu:client:validate": "concurrently \"npm run docu:client:validate:mixin\" \"npm run docu:client:validate:lifecycle\" \"npm run docu:client:validate:link\" \"npm run docu:client:validate:splitview\" \"npm run docu:client:validate:ux\" \"npm run docu:client:validate:storage\"",
    "docu:client:validate:mixin": "jsdoc2md --json ../client/luigi-client-wc-docu-mixin.ts",
    "docu:client:validate:lifecycle": "jsdoc2md --json ../client/src/lifecycleManager.ts",
    "docu:client:validate:link": "jsdoc2md --json ../client/src/linkManager.ts",
    "docu:client:validate:splitview": "jsdoc2md --json ../client/src/splitViewHandle.ts",
    "docu:client:validate:ux": "jsdoc2md --json ../client/src/uxManager.ts",
    "docu:client:validate:storage": "jsdoc2md --json ../client/src/storageManager.ts",
    "docu:client:generate:section": "jsdoc2md --partial ./docs/*.hbs --helper ./docs/mdHelpers.js --template ../docs/templates/luigi-client-api.md --separators --param-list-format=list --files ../client/luigi-client-wc-docu-mixin.ts ../client/src/lifecycleManager.ts ../client/src/linkManager.ts ../client/src/splitViewHandle.ts ../client/src/uxManager.ts ../client/src/storageManager.ts > ../docs/luigi-client-api.md",
    "docu:element": "jsdoc2md --partial ./docs/*.hbs --helper ./docs/mdHelpers.js --template ../docs/templates/luigi-element-api.md --param-list-format=list --files ../client/src/typings/luigi-element-api.d.ts ../client/src/typings/luigi-element-html.d.ts --configure ./docs/jsdoc2md.json > ../docs/luigi-element-api.md",
    "docu:element:validate": "jsdoc2md --configure ./docs/jsdoc2md.json --json ../client/src/typings/luigi-element-api.d.ts",
    "docu:core": "npm run docu:core:validate && npm run docu:core:generate:sections",
    "docu:core:validate": "concurrently \"npm run docu:core:validate:config\" \"npm run docu:core:validate:elements\" \"npm run docu:core:validate:navigation\" \"npm run docu:core:validate:i18n\" \"npm run docu:core:validate:messages\" \"npm run docu:core:validate:ux\" \"npm run docu:core:validate:globalsearch\" \"npm run docu:core:validate:theming\" \"npm run docu:core:validate:featuretoggles\" \"npm run docu:core:validate:routing\" \"npm run docu:core:validate:auth\"",
    "docu:core:validate:config": "jsdoc2md --json ../core/src/core-api/config.js",
    "docu:core:validate:elements": "jsdoc2md --json ../core/src/core-api/dom-elements.js",
    "docu:core:validate:navigation": "jsdoc2md --json ../core/src/core-api/navigation.js",
    "docu:core:validate:i18n": "jsdoc2md --json ../core/src/core-api/i18n.js",
    "docu:core:validate:messages": "jsdoc2md --json ../core/src/core-api/custom-messages.js",
    "docu:core:validate:ux": "jsdoc2md --json ../core/src/core-api/ux.js",
    "docu:core:validate:globalsearch": "jsdoc2md --json ../core/src/core-api/globalsearch.js",
    "docu:core:validate:theming": "jsdoc2md --json ../core/src/core-api/theming.js",
    "docu:core:validate:featuretoggles": "jsdoc2md --json ../core/src/core-api/featuretoggles.js",
    "docu:core:validate:routing": "jsdoc2md --json ../core/src/core-api/routing.js",
    "docu:core:validate:auth": "jsdoc2md --json ../core/src/core-api/auth.js",
    "docu:core:generate:sections": "jsdoc2md --partial ./docs/*.hbs --helper ./docs/mdHelpers.js --template ../docs/templates/luigi-core-api.md --separators --param-list-format=list --files ../core/src/core-api/config.js ../core/src/core-api/dom-elements.js ../core/src/core-api/navigation.js ../core/src/core-api/i18n.js ../core/src/core-api/custom-messages.js ../core/src/core-api/ux.js ../core/src/core-api/globalsearch.js ../core/src/core-api/theming.js ../core/src/core-api/featuretoggles.js ../core/src/core-api/routing.js ../core/src/core-api/auth.js > ../docs/luigi-core-api.md",
    "release": "babel-node tools/release-cli/release-cli.js",
    "publish:nightly": "babel-node tools/publish-nightly.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "cli-color": "^2.0.4",
    "concurrently": "^9.2.1",
    "diff": "^4.0.2",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^9.1.3",
    "prompts": "^2.4.2",
    "semver": "^7.7.3",
    "typescript": "^5.9.3"
  },
  "browserslist": "> 0.25%, not dead",
  "engines": {
    "node": ">=20.19.0"
  }
}
